{{form_start(form) }}
    <div class="input-group input-group-outline mb-3">
        <label class="form-label">Cni</label>
        {{form_widget (form.cni) }}
    </div>
    <div class="input-group input-group-outline mb-3">
        <label class="form-label">Nom</label>
        {{form_widget (form.nom) }}
    </div>
    <div class="input-group input-group-outline mb-3">
        <label class="form-label">Prenom</label>
            {{form_widget (form.prenom) }}
    </div>
        <div class="input-group input-group-outline mb-3">
        <label class="form-label">Telephone</label>
            {{form_widget (form.telephone) }}
    </div>
    <div class="input-group input-group-outline mb-3">
        <label class="form-label">nom utilisateur</label>
        {{form_widget (form.nom_utilisateur) }}
    </div>
    <div class="input-group input-group-outline mb-3">
        <label class="form-label">Mot de passe</label>
        {{form_widget (form.mot_de_passe) }}
    </div>
    
    
    <div class="text-center">
        <input type="submit" class="btn btn-lg bg-gradient-primary btn-lg w-100 mt-4 mb-0" value="{{ button_label|default('Save') }}">
    </div>
{{ form_end(form)}}